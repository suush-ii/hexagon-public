/* ~/CSS/ide/fonts/SourceSansPro.css */
@font-face {
	font-family: 'Source Sans Pro';
	font-style: normal;
	font-weight: 300;
	src:
		local('Source Sans Pro Light'),
		local('SourceSansPro-Light'),
		url('/ide/fonts/source-sans-pro-v9-latin-300.woff2') format('woff2'),
		url('/ide/fonts/source-sans-pro-v9-latin-300.woff') format('woff'),
		url('/ide/fonts/source-sans-pro-v9-latin-300.svg#SourceSansPro') format('svg');
}
@font-face {
	font-family: 'Source Sans Pro';
	font-style: normal;
	font-weight: 400;
	src:
		local('Source Sans Pro'),
		local('SourceSansPro-Regular'),
		url('/ide/fonts/source-sans-pro-v9-latin-regular.woff2') format('woff2'),
		url('/ide/fonts/source-sans-pro-v9-latin-regular.woff') format('woff'),
		url('/ide/fonts/source-sans-pro-v9-latin-regular.svg#SourceSansPro') format('svg');
}
@font-face {
	font-family: 'Source Sans Pro';
	font-style: normal;
	font-weight: 600;
	src:
		local('Source Sans Pro Semibold'),
		local('SourceSansPro-Semibold'),
		url('/ide/fonts/source-sans-pro-v9-latin-600.woff2') format('woff2'),
		url('/ide/fonts/source-sans-pro-v9-latin-600.woff') format('woff'),
		url('/ide/fonts/source-sans-pro-v9-latin-600.svg#SourceSansPro') format('svg');
}
/* ~/CSS/Pages/IDE/ClientToolBox2014.css */
html,
body {
	margin: 0;
	padding: 0;
	font-family: 'Source Sans Pro', sans-serif;
	font-size: 14px;
	overflow-x: hidden;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	user-select: none;
}
input[disabled] {
	background-color: #ccc;
	border: 1px dotted #666;
	color: #666;
	cursor: default;
}
.client-toolbox-header {
	background-color: #eee;
	border: 1px dotted #ccc;
	padding: 8px;
}
.client-toolbox-content {
	width: 100%;
	margin: 0 auto;
	padding-bottom: 10px;
	min-height: 420px;
}
.client-toolbox-label {
	font-size: 1em;
	font-weight: bold;
	margin: 12px 0;
	padding: 0 10px 0 0;
	display: inline-block;
}
.client-toolbox-header-bar {
	margin-left: auto;
	margin-right: auto;
	height: 27px;
}
.client-toolbox-searchinput {
	height: 22px;
	border: 1px solid #b8b8b8;
	border-right: 0;
	width: 100%;
	padding-left: 5px;
}
.client-toolbox-selection {
	padding: 4px 4px 4px 0;
	line-height: 22px;
	float: left;
	overflow: hidden;
	white-space: pre;
	text-overflow: ellipsis;
	margin-right: 5px;
	max-width: 45%;
	height: 25px;
}
.client-toolbox-subnav-selection {
	width: 118px;
	height: 30px;
	padding: 5px;
	line-height: 22px;
}
.client-input-wrapper {
	overflow: hidden;
}
.client-toolbox-search {
	width: auto;
	height: 27px;
}
.client-toolbox-search-btn {
	font-weight: bold;
	height: 26px;
	cursor: pointer;
	width: 17px;
	background: url('/ide/assets/btn-search_glass.png') right no-repeat;
	border: 0;
	border-right: 1px solid #b8b8b8;
	background-color: white;
	border: 1px solid #b8b8b8;
	border-left: 0;
	float: right;
	position: relative;
}
.client-toolbox-sort-selection {
	width: 104px;
}
.client-toolbox-selection option {
	line-height: 30px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	width: auto;
}
.client-toolbox-loading {
	margin: 20px auto;
	text-align: center;
	height: 17px;
	width: 48px;
	background: transparent url('/ide/assets/ProgressIndicator4.gif') no-repeat center;
}
.client-toolbox-referral-links {
	margin-left: auto;
	margin-right: auto;
	width: 100%;
	margin: 16px 0 16px 0;
	text-align: center;
}
.client-toolbox-referral-links a {
	padding: 0 2px 0 2px;
}
.client-toolbox-referral-links a:hover {
	cursor: pointer;
}
.client-toolbox-subnav {
	margin: 10px 5px 20px 5px;
}
.client-toolbox-subnav:before,
.client-toolbox-subnav:after {
	content: ' ';
	display: table;
}
.client-toolbox-subnav:after {
	clear: both;
}
.client-toolbox-sort,
.client-toolbox-paging {
	text-align: center;
}
.client-toolbox-paging-btn {
	height: 30px;
	margin: 10px 0;
	cursor: pointer;
}
.loading {
	display: block;
}
.client-toolbox-message {
	margin: 0;
	font-size: 14px;
	color: #888;
	text-align: center;
}
.client-toolbox-assets,
.client-toolbox-item {
	list-style: none;
	margin: 0;
	padding: 0;
}
.client-toolbox-assets {
	margin: 0 auto;
	padding-bottom: 25px;
	text-align: center;
	position: relative;
}
.client-toolbox-item {
	display: inline-block;
	position: relative;
	width: 75px;
	height: 120px;
	margin: 5px;
	z-index: 0;
	text-align: center;
	vertical-align: top;
}
.client-toolbox-image {
	margin-left: -3px;
	width: 81px;
	height: 100px;
	overflow: hidden;
}
.client-toolbox-inner {
	width: 75px;
	min-height: 115px;
	-webkit-transition:
		height 0.1s ease-in-out,
		display 0.3s ease-in-out 0.3s;
	-moz-transition:
		height 0.1s ease-in-out,
		display 0.3s ease-in-out 0.3s;
	-o-transition:
		height 0.1s ease-in-out,
		display 0.3s ease-in-out 0.3s;
	transition:
		height 0.1s ease-in-out,
		display 0.3s ease-in-out 0.3s;
}
.client-toolbox-asset-img {
	padding-top: 3px;
	display: block;
	width: 75px;
	height: 75px;
	margin: 0 auto;
	cursor: pointer;
}
.client-toolbox-asset-name {
	color: #4169e1;
	margin: 5px auto;
	display: block;
	height: 32px;
	overflow: hidden;
	white-space: normal;
	line-height: 15px;
	text-align: left;
	cursor: pointer;
}
.client-toolbox-creator {
	margin: 1px auto;
	padding: 0;
	display: none;
	line-height: 14px;
	color: #666;
	cursor: pointer;
	width: 75px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.client-toolbox-creator:hover {
	color: #000;
	text-decoration: underline;
}
.client-toolbox-vote-container {
	display: none;
	height: 30px;
	color: #666;
}
.client-toolbox-voting {
	display: none;
}
.client-toolbox-vote {
	background: transparent url('/images/Icons/thumbsup.png') no-repeat center;
	display: inline-block;
	height: 21px;
	width: 21px;
}
.client-toolbox-stamp {
	width: 20px;
	height: 20px;
	float: right;
	position: relative;
	top: -15px;
	right: 12px;
	background: transparent url('/images/icons/stamp-of-approval-v2.png');
}
.client-toolbox-audio {
	background-image: url(/images/Shared/generic_07172017.svg);
	background-position: 0 -308px;
	background-repeat: no-repeat;
	background-size: 56px auto;
	height: 28px;
	width: 28px;
	vertical-align: middle;
	position: relative;
	top: -67px;
	right: -52px;
}
.client-toolbox-audio:hover {
	cursor: pointer;
	background-position: -28px -308px;
}
.client-toolbox-audio.playing {
	background-position: 0 -336px;
}
.client-toolbox-audio.playing:hover {
	background-position: -28px -336px;
}
.client-toolbox-upvote {
	background-position: 0 -63px;
}
.client-toolbox-downvote {
	background-position: 0 -128px;
}
.client-toolbox-upvote:hover {
	background-position: 0 -21px;
	cursor: pointer;
}
.client-toolbox-upvote.client-toolbox-vote-selected {
	background-position: 0 -41px;
}
.client-toolbox-downvote:hover {
	background-position: 0 -106px;
	cursor: pointer;
}
.client-toolbox-downvote.client-toolbox-vote-selected {
	background-position: 0 -149px;
}
.client-toolbox-progress {
	background-color: #ce645b;
	height: 5px;
	margin: 5px auto;
	position: relative;
}
.client-toolbox-bar {
	background-color: #52a846;
	position: absolute;
	bottom: 0;
	left: 0;
	height: 5px;
}
.client-toolbox-item:hover {
	z-index: 100;
}
.client-toolbox-item:hover .client-toolbox-inner {
	padding: 9px 8px;
	z-index: 100;
	position: absolute;
	top: -10px;
	left: -9px;
	background: rgba(255, 255, 255, 0.9);
	border: 1px solid #ccc;
}
.client-toolbox-item:hover p {
	z-index: 100;
	display: inline-block;
}
.client-toolbox-item:hover .client-toolbox-vote-container {
	z-index: 100;
	display: inline-block;
}
.client-toolbox-item:hover .client-toolbox-voting {
	z-index: 100;
	display: block;
}
.client-toolbox-noresult {
	text-align: center;
	background-color: #ccc;
	height: 30px;
	padding: 10px;
}
.client-toolbox-error-message {
	text-align: center;
	background-color: #feefb3;
	height: 30px;
	padding: 10px;
}
.client-toolbox-bg-selector {
	display: inline-block;
	vertical-align: middle;
	width: 16px;
	height: 16px;
	margin: 2px;
	cursor: pointer;
	border-radius: 50%;
	border: 2px solid transparent;
}
.client-toolbox-bg-selector.selected {
	border: 2px solid #2196f3;
}
.client-toolbox-bg-checkered {
	background-image: url('/images/IDE/toolbox-bg-checkered.png');
}
.client-toolbox-bg-white {
	background-color: white;
}
.client-toolbox-bg-black {
	background-color: black;
}
.client-toolbox-footer {
	border-top: 1px solid #b8b8b8;
	background-color: #e3e3e3;
	height: 23px;
	position: fixed;
	width: 100%;
	bottom: 0;
	z-index: 300;
	padding: 6px;
}
.client-toolbox-footer div {
	float: left;
	color: #191919;
	font-size: 12px;
	line-height: 1em;
}
.client-toolbox-lbl {
	line-height: 24px;
	float: left;
	font-size: 11px;
}
.client-toolbox-color-chip {
	width: 10px;
	height: 10px;
	margin-right: 6px;
	border: 1px solid #b8b8b8;
	background-color: #fff;
}
.client-toolbox-color-chip.black {
	border-color: #191919;
	background-color: #191919;
}
.client-toolbox-btn-color {
	display: flex;
	min-width: 52px;
	height: 17px;
	margin-left: 6px;
	padding-left: 5px;
	float: left;
	border: 1px solid #b8b8b8;
	background-color: #fff;
	border-radius: 3px;
	font-size: 12px;
	line-height: 1em;
	text-decoration: none;
	padding-top: 4px;
}
.client-toolbox-btn-color:focus {
	outline: none;
}
.client-toolbox-btn-color:hover {
	border-color: #00a2ff;
}
.client-toolbox-btn-color.selected {
	border-color: #00a2ff;
	background-color: #00a2ff;
	color: #fff;
}
.client-toolbox-btn-color.selected div {
	color: #fff;
	float: left;
}
.small-chip.white {
	background-color: #fff;
}
.small-chip {
	width: 50%;
	height: 50%;
	float: left;
	background-color: #b8b8b8;
}
