{
	"name": "hexagon",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "drizzle-kit generate:pg",
		"push": "drizzle-kit push:pg",
		"drop": "drizzle-kit drop",
		"prod": "PROTOCOL_HEADER=x-forwarded-proto HOST_HEADER=x-forwarded-host PORT=9000 HOST=127.0.0.1 node build",
		"prodpm2": "PROTOCOL_HEADER=x-forwarded-proto HOST_HEADER=x-forwarded-host PORT=9000 HOST=127.0.0.1 pm2 start build/index.js",
		"prodpm2reload": "PROTOCOL_HEADER=x-forwarded-proto HOST_HEADER=x-forwarded-host PORT=9000 HOST=127.0.0.1 pm2 reload build/index.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/adapter-node": "^2.0.2",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/node": "^20.10.6",
		"@typescript-eslint/eslint-plugin": "^6.18.0",
		"@typescript-eslint/parser": "^6.18.0",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.20.9",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"formsnap": "^1.0.0",
		"postcss": "^8.4.33",
		"postcss-load-config": "^5.0.2",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"sveltekit-superforms": "^2.12.4",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.485.0",
		"@aws-sdk/s3-request-presigner": "^3.485.0",
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"@lucia-auth/oauth": "^3.5.1",
		"@sentry/sveltekit": "^7.111.0",
		"@threlte/core": "^7.3.0",
		"@threlte/extras": "^8.11.2",
		"@types/three": "^0.163.0",
		"@yaireo/relative-time": "^1.0.4",
		"bits-ui": "^0.21.4",
		"clsx": "^2.1.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.30.10",
		"jose": "^5.2.4",
		"lucia": "^2.7.7",
		"lucide-svelte": "^0.372.0",
		"mode-watcher": "^0.3.0",
		"postgres": "^3.4.3",
		"svelte-adapter-bun": "^0.5.1",
		"svelte-sonner": "^0.3.22",
		"tailwind-merge": "^2.2.0",
		"tailwind-variants": "^0.1.20",
		"three": "^0.163.0"
	}
}
