{
	"name": "hexagon",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "drizzle-kit generate:pg",
		"push": "drizzle-kit push:pg",
		"drop": "drizzle-kit drop",
		"prod": "PROTOCOL_HEADER=x-forwarded-proto HOST_HEADER=x-forwarded-host PORT=9000 HOST=127.0.0.1 node build",
		"prodpm2": "PROTOCOL_HEADER=x-forwarded-proto HOST_HEADER=x-forwarded-host PORT=9000 HOST=127.0.0.1 pm2 start build/index.js",
		"prodpm2reload": "PROTOCOL_HEADER=x-forwarded-proto HOST_HEADER=x-forwarded-host PORT=9000 HOST=127.0.0.1 pm2 reload build/index.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^2.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^20.8.10",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.34.0",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"sveltekit-superforms": "^1.9.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.441.0",
		"@aws-sdk/s3-request-presigner": "^3.441.0",
		"@lucia-auth/adapter-postgresql": "^2.0.1",
		"@lucia-auth/oauth": "^3.3.1",
		"@sentry/sveltekit": "^7.77.0",
		"bits-ui": "^0.5.7",
		"clsx": "^2.0.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.6",
		"formsnap": "^0.1.4",
		"lucia": "^2.7.2",
		"lucide-svelte": "^0.291.0",
		"postgres": "^3.4.2",
		"svelte-adapter-bun": "^0.5.0",
		"tailwind-merge": "^1.14.0",
		"tailwind-variants": "^0.1.18"
	}
}